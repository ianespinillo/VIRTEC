{
	"compilerOptions": {
		"module": "NodeNext",
		"moduleDetection": "force",
		"noUncheckedIndexedAccess": true,
		"resolveJsonModule": true,
		"target": "ES2022",
		"strict": true,
		"jsx": "preserve",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "nodenext",
		"isolatedModules": true,
		"baseUrl": "."
	}
}
