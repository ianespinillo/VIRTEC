{
	"name": "api",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"dev": "nest start --watch --preserveWatchOutput",
		"start:dev:seed": "cross-env SEED_MODE=true nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"db:push": "prisma db push",
		"db:pull": "prisma db pull && prisma generate",
		"db-build": "prisma generate && prisma db push",
		"db-seed": "prisma db seed",
		"db-reset": "ts-node prisma/truncate.ts",
		"lint:format": "bunx @biomejs/biome format --write .",
		"lint:fix": "bunx @biomejs/biome check --apply .",
		"lint:check": "bunx @biomejs/biome check .",
		"prepare": "husky",
		"ts:check": "tsc --noEmit"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts",
		"reset": "ts-node prisma/truncate.ts"
	},
	"dependencies": {
		"@golevelup/ts-jest": "^0.5.4",
		"@nestjs-modules/mailer": "^2.0.2",
		"@nestjs/common": "^10.0.0",
		"@nestjs/config": "^3.2.3",
		"@nestjs/core": "^10.0.0",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/mapped-types": "*",
		"@nestjs/passport": "^10.0.3",
		"@nestjs/platform-express": "^10.0.0",
		"@prisma/client": "^5.16.1",
		"@repo/common": "workspace:*",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"cloudinary": "^2.3.0",
		"commitlint": "^19.3.0",
		"cookie-parser": "^1.4.7",
		"env-var": "^7.5.0",
		"handlebars": "^4.7.8",
		"hbs": "^4.2.0",
		"husky": "^9.1.1",
		"jest-mock-extended": "^3.0.7",
		"jsonwebtoken": "^9.0.2",
		"nodemailer": "^6.9.14",
		"passport-jwt": "^4.0.1",
		"passport-token": "^0.2.0",
		"prisma": "^5.16.1",
		"reflect-metadata": "^0.2.0",
		"rxjs": "^7.8.1",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.5.2",
		"@types/multer": "^1.4.12",
		"@types/node": "^20.3.1",
		"@types/supertest": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"jest": "^29.5.0",
		"prettier": "^3.0.0",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.0",
		"ts-loader": "^9.4.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.3"
	}
}
